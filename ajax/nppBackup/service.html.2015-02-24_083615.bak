<script>
// Create fancybox2 gallery
function DemoGallery(){
	$('.fancybox').fancybox({
		openEffect	: 'none',
		closeEffect	: 'none'
	});
}
$(document).ready(function() {
	// Load Fancybox2 and make gallery in callback
	LoadFancyboxScript(DemoGallery);
	formService()
	
	function formService(){
  $('#simple_gallery div').on('click', function(){
		var header = 'commander';
		var form = $('<form id="serviceForm" method="post" action="" class="form-horizontal col-lg-10">'+'<div class="row"><div class="form-group"><label class="col-lg-5 col-md-7">Nom et Prenom : </label><div class="col-lg-10"><input type="text" class="form-control" name="username" /></div></div></div><div class="row"><div class="form-group"><label col-lg-5 col-md-7">Type de Site</label><div class="col-lg-10"><input type="text" class="form-control" name="siteType" /></div></div></div><div class="row"><div class="form-group"><label class="col-lg-5 col-md-7">Téléphone</label><div class="col-sm-7 col-lg-10"><input type="text" class="form-control" name="tel" /></div></div></div><div class="row"><div class="form-group"><label class="col-lg-5 col-md-7">Email</label><div class="col-sm-7 col-lg-10"><input type="text" class="form-control" name="mail" /></div></div></div><div class="row"><div class="form-group"><label class="col-lg-5 col-md-7">Objet</label><div class="col-sm-7 col-lg-10"><input type="text" class="form-control" name="objet" /></div></div></div><div class="row"><div class="form-group"><label for="textarea" class="col-lg-5 col-md-7">Description </label><textarea id="textarea" rows="4" class="form-control"></textarea><div class="col-lg-10"></div></div></div></form>');
		var button = $('<button class="btn btn-primary pullright" type="submit">Envoyer</button><div class="alert alert-block alert-danger" style="display: none"><h4>Erreur ! </h4>Vous devez entrer au moins 4 caractères !</div>');
		OpenModalBox(header, form, button);
	});

 }
	
$( function( ) {
   $( "form").on( "submit", function( ) {
     if ($("input").val( ).length < 4) {
       $("div.form-group").addClass("has-error") ;
       $("div.alert").show("slow").delay(4000).hide("slow") ;
       return false;
     }
  } ) ;
} ) ;
	
});
</script>

